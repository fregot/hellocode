#include<stdio.h>//140.3
int main()
{
  int n,m;
  printf("Enter m and n:");
  scanf("%d%d",&m,&n);
  int t;
  if(m>n)
    {
      t=m;
      m=n;
      n=t;
    }
  for(int i=m;i>=0;i--)
    {
      if(m%i==0&&n%i==0)
	{
	  printf("zui da gong yue shu:%d\n",i);
	  printf("zui xiao gong bai shu :%d\n",m*n/i);
	  break;
	}
    }
  return 0;
}
